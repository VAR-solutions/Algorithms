 
#include <stdio.h> 

/* function to print an array */
void printArray(int arr[], int size); 

/*Fuction to get gcd of a and b*/
int gcd(int a, int b); 

/*Function to left rotate arr[] of siz n by d*/
void leftRotate(int arr[], int d, int n) 
{ 
	int i, j, k, temp; 
	/* To handle if d >= n */
	d = d % n; 
	int g_c_d = gcd(d, n); 
	for (i = 0; i < g_c_d; i++) { 
		/* move i-th values of blocks */
		temp = arr[i]; 
		j = i; 
		while (1) { 
			k = j + d; 
			if (k >= n) 
				k = k - n; 
			if (k == i) 
				break; 
			arr[j] = arr[k]; 
			j = k; 
		} 
		arr[j] = temp; 
	} 
} 


/* function to print an array */
void printArray(int arr[], int n) 
{ 
	int i; 
	for (i = 0; i < n; i++) 
		printf("%d ", arr[i]); 
} 

/*Fuction to get gcd of a and b*/
//  the folowing function is used to find the gcd of two numbers using euclid algorithm.
int gcd(int a, int b) 
{ 
	if (b == 0) 
		return a; 
	else
		return gcd(b, a % b); 
} 


int main() 
{ 
    
    int n; //enter no_of_elements.
    scanf("%d",&n);
    int d; // By how many digits u want to left rotate the array.
    scanf("%d",&d);
	int arr[n]; 
	for(int i=0;i<n;i++)
	scanf("%d",&arr[i]);
	leftRotate(arr, d, n); 
	printArray(arr, n); 
	getchar(); 
	return 0; 
} 

