#include<stdio.h>

int main()
{
	int a[100],b[100],c[100],k=0,i,j,n;
	
	printf("Enter size: ");
	scanf("%d",&n);
	
	printf("Enter array elements:\n");
	for(i=1;i<=n;i++)
		scanf("%d",&a[i]);
	
	for(i=1;i<=n;i++)
	{
		if(a[i]>k)
			k=a[i];
	}
	
	for(i=0;i<=k;i++)
		c[i]=0;
	for(j=1;j<=n;j++)
		c[a[j]]=c[a[j]]+1;
	for(i=1;i<=k;i++)
		c[i]=c[i]+c[i-1];
	for(j=n;j>0;j--)
	{
		b[c[a[j]]]=a[j];
		c[a[j]]=c[a[j]]-1;
	}
	
	printf("Sorted array--->\n");
	for(i=1;i<=n;i++)
		printf("%d\n",b[i]);
	
	return 0;
}
